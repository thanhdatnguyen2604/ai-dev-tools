{
  "name": "02-coding-interview",
  "version": "1.0.0",
  "description": "A minimal but working real-time collaborative coding interview platform built with Django (backend) and React (frontend).",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "concurrently --names \"BACKEND,FRONTEND\" --prefix-colors \"blue,green\" \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && ./venv/bin/daphne -b 0.0.0.0 -p 8989 coding_interview_backend.asgi:application",
    "dev:backend:windows": "cd backend && venv\\Scripts\\daphne.exe -b 0.0.0.0 -p 8989 coding_interview_backend.asgi:application",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:quick": "./start-dev.sh"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
